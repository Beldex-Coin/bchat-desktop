{
  "defaultSeverity": "error",
  "extends": ["../../tslint.json"],
  "rules": {
    "no-implicit-dependencies": false,
    "max-func-body-length": false,
    "no-unused-expression": false,
    "no-unused-variable": false,
    "await-promise": [true, "PromiseLike"],
    "no-floating-promises": [true, "PromiseLike"]
  }
}
